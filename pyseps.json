{
  "system":{
    "metrics":true,
    "metrics_interval":10,
    "metrics_dst":"logging"
  },
  "bootstrap":{
    "queries":{
       "module":"wishbone.iomodules.broker",
       "class":"Broker",
       "variables":{
          "host":"localhost",
          "vhost":"/",
          "username":"guest",
          "password":"guest",
          "consume_queue":"pyseps:queries",
          "prefetch_count":1,
          "no_ack":false,
          "delivery_mode":2,
          "auto_create":true
       }
    },
    "validate":{
         "module":"wishbone.modules.jsonvalidator",
         "class":"JSONValidator",
         "variables":{
            "schema":false,
            "convert":true
         }
    },
    "tail":{
      "module":"pyseps.tail",
      "class":"Tail",
      "variables":{
        "host":"localhost",
        "db":"mongotails",
        "collection":"input",
        "query_file":"queries.ini"
      }
    },
    "stdout":{
      "module":"wishbone.modules.stdout",
      "class":"STDOUT",
      "variables":{
        "purge":true,
        "counter":false
      }
    }
  },
  "routingtable":{
    "queries.inbox":[
      "validate.inbox"
    ],
    "validate.outbox":[
      "tail.queries"
    ],
    "tail.acknowledge":[
      "queries.acknowledge"
    ],
    "tail.outbox":[
      "stdout.inbox"
    ]
  }
}
