{
  "system":{
    "metrics":true,
    "metrics_interval":1,
    "metrics_dst":"table"
  },
  "bootstrap":{
    "queries":{
       "module":"wishbone.iomodules.broker",
       "class":"Broker",
       "variables":{
          "host":"localhost",
          "vhost":"/",
          "username":"guest",
          "password":"guest",
          "consume_queue":"pyseps:queries",
          "prefetch_count":1,
          "no_ack":false,
          "delivery_mode":2,
          "auto_create":true
       }
    },
    "queries_validate":{
         "module":"wishbone.modules.jsonvalidator",
         "class":"JSONValidator",
         "variables":{
            "schema":false,
            "convert":true
         }
    },
    "logs_tail":{
      "module":"pyseps.tail",
      "class":"Tail",
      "variables":{
        "host":"localhost",
        "db":"mongotails",
        "collection":"input"
      }
    }
  },
  "routingtable":{
    "queries.inbox":[
      "queries_validate.inbox"
    ],
    "queries_validate.outbox":[
      "logs_tail.queries"
    ],
    "logs_tail.acknowledge":[
      "queries.acknowledge"
    ],
    "logs_tail.inbox":[
      "queries.outbox"
    ]
  }
}
